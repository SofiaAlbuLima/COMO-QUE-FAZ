      <section class="conteudo-dispositivo desktop">
         <section class="titulo-linha-pagina">
            <img src="/imagens-svg/5-adm/iconRoxoPagina-premium.svg" alt="" class="imagem-pagina-premium">
            <h1 class="titulo-pagina">PÁGINA | ACESSO PREMIUM</h1>
