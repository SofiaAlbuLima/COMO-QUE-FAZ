<section class="postagens">
    <% posts.forEach(function(post) { %>
       <section class="postagem">
          <figure class="postagem_parte_de_cima">
             <h1 id="texto-a   tras5-B" class="titulo_post postagem_info_primeiro_plano"><%= post.nome %></h1>
             <img id="icon-categoria-branco" src="imagens/icon-bemestar-branco.png" class="postagem_info_primeiro_plano">
             <img src="imagens/post-organizacao-rotina.png" alt="foto-post5" class="imagem_postagem">
          </figure>
             <aside class="legenda">
                <figure class="legenda relogio">
                   <img src="imagens/relogio-cinza.svg" alt="relogio">
                   <h3><%= post.tempo %></h3>
                </figure>
                <figure class="legenda estrela">
                   <img src="imagens/estrela-cinza.svg" alt="estrela">
                   <h3>5.0</h3>
                </figure>
                <% if(post.porcoes != null){ %>
                <figure class="legenda partes">
                   <img src="imagens/partes-cinza.png" alt="partes">
                   <h3><%= post.porcoes %></h3>
                </figure>
                <% } %>
             </aside>
             <aside class="hover-post" id="hover-post5-B">
                <h3>essa dica não possui itens necessários</h3>
             </aside>
    
       </section>
    <% }); %>
 </section>
    <% if (paginador) { %>
       <div class="notificador">
          <ul>
             <% for(var i=1; i <=paginador.total_paginas; i++){
                if(paginador.pagina_atual==i){
             %>
             <li><a class="pagina-atual" href="?pagina=<%= i %>">
                   <%= i %>
                </a>
             </li>
             <% } else { %> 
                <li><a href="?pagina=<%= i %>">
                      <%= i %>
                   </a>
                </li>
             <% }
          } %> 
          </ul>
       </div>
    <% }; %>
 </section>